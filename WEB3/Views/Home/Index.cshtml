<main class="main">

    <!-- Hero Section -->
    <section id="hero" class="hero section dark-background">

        <img src="/assets/img/salon/model.jpg" alt="" data-aos="fade-in">

        <div class="container">
            <div class="row">
                <div class="col-lg-8 d-flex flex-column align-items-center align-items-lg-start">
                    <h2 data-aos="fade-up" data-aos-delay="100">Welcome to <span> Sirius</span> <br /></h2>
                    <h4 style="margin-left:230px">Beauty Salon & Spa</h4>
                    <p data-aos="fade-up" data-aos-delay="200">"Feel Beautiful, Be Sirius"</p>
                    <div class="d-flex mt-4" data-aos="fade-up" data-aos-delay="300">
